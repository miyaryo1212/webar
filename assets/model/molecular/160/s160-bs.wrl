PROTO Atom [
  field SFVec3f Pos 0 0 0
  field SFColor Color 0.216 0.216 0.216
  field SFFloat Radius   0.480
  field SFFloat Transp 0
]{
  Transform{
    translation IS Pos
    children[
      Shape{
        appearance Appearance{
          material Material{
            diffuseColor IS Color
            specularColor 1 1 1
            transparency IS Transp
            shininess 0.8
          }
        }
        geometry Sphere{
          radius IS Radius
        }
      }
    ]
  }
}

PROTO Bond [
  field SFVec3f Pos 0 0 0
  field SFColor Color 0.700 0.700 0.700
  field SFRotation Rot 0 0 0 0
  field SFFloat Len 1.0
  field SFFloat Radius   0.100
  field SFFloat Transp 0
]{
  Transform{
    translation IS Pos
    rotation IS Rot
    children[
      Shape{
        appearance Appearance{
          material Material{
            diffuseColor IS Color
            specularColor 1 1 1
            transparency IS Transp
            shininess 0.8
          }
        }
        geometry Cylinder{
          radius IS Radius
          height IS Len
        }
      }
    ]
  }
}


